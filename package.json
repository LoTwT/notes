{
  "name": "notes",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.12",
  "description": "",
  "author": "LoTwT",
  "license": "ISC",
  "homepage": "https://github.com/LoTwT/notes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/notes.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/notes/issues"
  },
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "dev": "TARGET=dev vitepress dev --open",
    "build": "TARGET=prod vitepress build && tsx scripts/copy.ts",
    "preview": "TARGET=prod vitepress preview",
    "sum:al": "tsx scripts/sumOfAlgorithm.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "vite-plugin-sirv": "^0.0.2"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.2.7",
    "@ayingott/sucrose": "^0.0.31",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.0",
    "eslint": "^8.47.0",
    "lint-staged": "^14.0.0",
    "pnpm": "^8.6.12",
    "prettier": "^3.0.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitepress": "1.0.0-rc.4",
    "vue": "^3.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@ayingott/eslint-config",
    "ignorePatterns": [
      "dist",
      "node_modules",
      ".vitepress/dist",
      ".vitepress/cache",
      "docs/toolchain/vite/prebuild.md",
      "docs/toolchain/vite/pluginPipeline.md",
      "docs/toolchain/vite/plugin.md",
      "docs/toolchain/vite/optimize.md",
      "docs/toolchain/vite/hmr.md",
      "docs/toolchain/vite/hmr2.md",
      "docs/toolchain/vite/config.md",
      "docs/toolchain/babel/traverse.md",
      "docs/toolchain/babel/API.md",
      "docs/compilation/code-analyze/analyze-import.md"
    ]
  }
}
